<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox spacing="8" xmlns="http://javafx.com/javafx/21.0.8" xmlns:fx="http://javafx.com/fxml" fx:controller="com.jobarbosa.gestortarefas.LoginController">
    <children>
        <Label text="Username:" />
        <TextField fx:id="usernameField" />
        <Label text="Password:" />
        <PasswordField fx:id="passwordField" />
        <Button text="Login" fx:id="loginButton" />
        <Button text="Register" fx:id="registerButton" />
        <Label fx:id="feedbackLabel" />
    </children>
</VBox>

